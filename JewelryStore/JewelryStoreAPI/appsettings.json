{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureKeyVaultUri": "https://jewelrystoreweb-0-kv.vault.azure.net/",
  "ConnectionStrings": {
    "JewelryStoreDatabase": "Connection string"
  },
  "JwtSettings": {
    "Secret": "Long private secret",
    "Lifetime": 0
  },
  "KafkaConfiguration": {
    "ServerUrl": "localhost:9092",
    "GroupId": "watches-consumer-group",
    "Topics": {
      "CreateWatch": "watches"
    }
  },
  "RedisConfiguration": {
    "ServerUrl": "localhost:6379",
    "CacheExpirationInSeconds": 10,
    "CacheKeys": {
      "GetAllWatches": "watches"
    }
  }
}
